{
  "name": "nuxt2-koa-template",
  "version": "1.1.0",
  "private": true,
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon app/index.js --watch app",
    "build": "nuxt build",
    "analyz": "nuxt build --analyze",
    "start": "cross-env NODE_ENV=production node app/index.js",
    "start:preprod": "cross-env NODE_ENV=preprod node app/index.js",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{vue,css}\" --ignore-path .gitignore",
    "lint": "yarn lint:js && yarn lint:style",
    "test": "jest",
    "commit": "git-cz",
    "release": "standard-version --release-as minor --no-verify",
    "release:patch": "standard-version --release-as patch --no-verify"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint",
    "*.{css,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.1",
    "ant-design-vue": "^1.7.6",
    "async-validator": "^4.0.1",
    "cookie-universal-nuxt": "^2.1.5",
    "core-js": "^3.9.1",
    "http-errors": "^2.0.0",
    "ioredis": "^5.2.1",
    "koa": "^2.13.1",
    "koa-bodyparser": "^4.3.0",
    "koa-compose": "^4.1.0",
    "koa-helmet": "^6.1.0",
    "koa-router": "^10.0.0",
    "koa2-cors": "^2.0.6",
    "log4js": "^6.6.0",
    "mysql2": "^2.3.3",
    "node-schedule": "^2.1.0",
    "normalize.css": "^8.0.1",
    "nuxt": "^2.15.3",
    "sequelize": "^6.21.3",
    "urllib": "^2.38.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@nuxtjs/eslint-config": "^6.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/style-resources": "^1.2.0",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@vue/test-utils": "^1.1.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-import": "^1.13.3",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.7.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "node-sass": "5.0.0",
    "nodemon": "^2.0.12",
    "prettier": "^2.2.1",
    "sass-loader": "10.1.1",
    "standard-version": "^9.3.0",
    "stylelint": "^13.12.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^21.0.0",
    "vconsole": "^3.8.1",
    "vue-jest": "^3.0.4"
  },
  "standard-version": {
    "scripts": {
      "posttag": "git push --follow-tags origin HEAD"
    }
  }
}
